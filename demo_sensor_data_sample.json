    "4:E4",
    "5:B3",
    "6:G#3",
    "7:F#3",
    "8:E3",
    "9:D3",
    "10:B2"
  ],
  "pedal_changes": [
    {
      "pedal": "A",
      "strings": [
        "5:+2st",
        "10:+2st"
      ]
    },
    {
      "pedal": "B",
      "strings": [
        "3:+1st",
        "6:+1st"
      ]
    },
    {
      "pedal": "C",
      "strings": [
        "4:+2st",
        "5:+2st"
      ]
    }
  ],
  "lever_changes": [
    {
      "lever": "LKL",
      "strings": [
        "4:-1st",
        "8:-1st"
      ]
    },
    {
      "lever": "LKR",
      "strings": [
        "4:+1st",
        "8:+1st"
      ]
    },
    {
      "lever": "LKV",
      "strings": [
        "(none on E9)"
      ]
    },
    {
      "lever": "RKL",
      "strings": [
        "1:+1st"
      ]
    },
    {
      "lever": "RKR",
      "strings": [
        "2:+2st"
      ]
    }
  ],
  "packet_schema": {
    "t_us": "timestamp in microseconds from start",
    "sens": "hall-effect sensor readings [f0, f5, f10, f15], range 0.000–1.000",
    "ped": "pedal engagement [A, B, C], range 0.000–1.000 (>0.5 = engaged)",
    "lev": "lever engagement [LKL, LKR, LKV, RKL, RKR], range 0.000–1.000 (>0.5 = engaged)",
    "vol": "volume pedal position, range 0.000–1.000",
    "picks": "per-string activation [str1..str10], boolean (from hex pickup RMS threshold)"
  },
